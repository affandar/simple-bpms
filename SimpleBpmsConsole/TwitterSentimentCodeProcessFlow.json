{
  "Name": "TwitterSentimentCodeProcessFlow",
  "Version": "1.0",
  "Trigger": {
    "Type": "Twitter",
    "TriggerData": {
      "hashtag": "ContosoComputers"
    }
  },
  "InputParameterBindings": null,
  "Nodes": [
    {
      "Id": 0,
      "TaskName": "SentimentAnalysisTask",
      "TaskVersion": "1.0",
      "NodeType": null,
      "InputParameterBindings": {
        "text": "%tweet_body%"
      },
      "ChildTaskIds": [
        1
      ],
      "ChildTaskSelectors": null
    },
    {
      "Id": 1,
      "TaskName": null,
      "TaskVersion": null,
      "NodeType": null,
      "InputParameterBindings": null,
      "ChildTaskIds": [
        2,
        3
      ],
      "ChildTaskSelectors": {
        "2": {
          "Key": "sentiment_score",
          "Operator": "GTE",
          "Value": "2"
        },
        "3": {
          "Key": "sentiment_score",
          "Operator": "LT",
          "Value": "0"
        }
      }
    },
    {
      "Id": 2,
      "TaskName": "EmailTask",
      "TaskVersion": "1.0",
      "NodeType": null,
      "InputParameterBindings": {
        "from": "bpms@bpms.org",
        "to": "affandar@microsoft.com",
        "subject": "New Sales Lead: @%tweet_from%",
        "body": "New sales lead. Sentiment Score: %sentiment_score% Tweet: %tweet_body%"
      },
      "ChildTaskIds": [
        5
      ],
      "ChildTaskSelectors": null
    },
    {
      "Id": 5,
      "TaskName": "SalesForceCreateRecordTask",
      "TaskVersion": "1.0",
      "NodeType": null,
      "InputParameterBindings": {
        "last_name": "@%tweet_from%",
        "description": "User @%tweet_from% expressed interest in AcmeComputers."
      },
      "ChildTaskIds": [
        6
      ],
      "ChildTaskSelectors": null
    },
    {
      "Id": 6,
      "TaskName": "KpiTask",
      "TaskVersion": "1.0",
      "NodeType": null,
      "InputParameterBindings": {
        "counter_name": "twitter_leads"
      },
      "ChildTaskIds": null,
      "ChildTaskSelectors": null
    },
    {
      "Id": 3,
      "TaskName": "HandleNegativeSentimentProcess",
      "TaskVersion": "1.0",
      "NodeType": 1,
      "InputParameterBindings": {
        "tweet_body": "%tweet_body%",
        "tweet_from": "%tweet_from%"
      },
      "ChildTaskIds": null,
      "ChildTaskSelectors": null
    }
  ]
}